# Public environment variables (accessible in client-side code)
PUBLIC_SUPABASE_URL=http://localhost:54321
PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here
PUBLIC_DEFAULT_LOCALE=en-US

# Server-only environment variables (never exposed to client)
SUPABASE_URL=http://localhost:54321
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Optional environment variables
OPENROUTER_API_KEY=

# Supabase Auth Email Templates (for local development)
# Note: These may be required if content_path in config.toml doesn't work due to Docker container isolation
# GOTRUE_MAILER_TEMPLATES_MAGIC_LINK=/path/to/supabase/templates/invite.html
# GOTRUE_MAILER_TEMPLATES_INVITE=/path/to/supabase/templates/tour-invite.html
